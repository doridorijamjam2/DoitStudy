<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.1//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.spring.studys.MemberDAO">	
	<select id="selectUser" resultType="com.spring.studys.UserDetailsVO">
		SELECT * FROM MEMBER WHERE ID = #{param1}
	</select>
	
	<select id="getAuthList" resultType="String">
		SELECT AUTHORITY FROM USERAUTH WHERE ID = #{param1}
	</select>
		
</mapper>